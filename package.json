    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "lint-staged": "^13.2.3",
    "npm-check-updates": "^16.10.16",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "puppeteer": "^20.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "reg-keygen-git-hash-plugin": "^0.12.2",
    "reg-notify-github-plugin": "^0.14.4",
    "reg-suit": "^0.12.2",
    "rimraf": "^5.0.1",
    "standard-version": "^9.5.0",

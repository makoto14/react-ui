    "react-dom": "^18.0.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "^18.2.15"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "cspell"
    ]
  },
  "volta": {
    "node": "20.12.2",
    "pnpm": "9.0.6"
  }
}
